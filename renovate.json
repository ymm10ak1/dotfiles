{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":disableDependencyDashboard",
    "github>aquaproj/aqua-renovate-config#2.7.5"
  ],
  "packageRules": [
      {
          "matchUpdateTypes": ["minor", "patch"],
          "matchCurrentVersion": "!/^0/",
          "automerge": true,
          "addLabels": ["automerge"]
      }
  ]
}
